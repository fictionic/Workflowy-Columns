{
  "manifest_version": 2,

  "name": "Workflowy Columns",
  "description": "View Workflowy lists as columns",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://workflowy.com/*"],
      "css": ["columns.css"],
      "js": ["jquery-3.2.1.min.js", "columns.js"],
      "run_at": "document_end"
    }
  ]
}
